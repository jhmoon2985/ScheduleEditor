<!-- Resources/Styles/IconStyles.xaml -->
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Insert/Add Icon (Plus) -->
    <Canvas x:Key="InsertIcon" Width="24" Height="24" Clip="F1 M 0,0L 24,0L 24,24L 0,24L 0,0">
